
#WHILE LOOP
#write a program to print hello every one 5 times using loop
{
  l=0
  {
  while (l<=5){
    print("manju")
    l=l+1;
  }
  }
}

#print a 1 to 5
i=1
while (i<=5){
  print(i)
  i=i+1
}

#print reverce order 5 to 1
i=5
while (i<=1){
  print(i)
  i=i-1
}

#write a program to print square  of even numbers from  1 to 10
i=1
while (i<=10){
  if (i%%2==0){
  print(i^2)}
  i=i+1
}
